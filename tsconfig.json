{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "outDir": "./dist",
    "strictNullChecks": true,
    "target": "es2019",
    "baseUrl": ".",
    "paths": {
      "@data/*": ["./src/data/*"],
      "@exercises/*": ["./src/exercises/*"],
      "@functions/*": ["./src/functions/*"],
      "@models/*": ["./src/models/*"]
    }
  }
}
